{
  "name": "ng-material-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build-angular-rating": "ng build angular-rating",
    "watch-angular-rating": "ng build angular-rating --watch",
    "build-angular-logging": "ng build angular-console-logger",
    "watch-angular-logging": "ng build angular-console-logger --watch",
    "lint-angular-rating": "ng lint angular-rating",
    "lint-angular-logging": "ng lint angular-console-logger",
    "lint-all-libs": "ng lint angular-rating && ng lint angular-console-logger",
    "test-angular-rating-watch": "ng test angular-rating --browsers=ChromeHeadless",
    "test-angular-logging-watch": "ng test angular-console-logger --browsers=ChromeHeadless",
    "test-all-libs-watch": "run-p test-angular-rating-watch test-angular-logging-watch",
    "test-angular-rating-ci": "ng test angular-rating --watch=false --browsers=ChromeHeadless",
    "test-angular-logging-ci": "ng test angular-console-logger --watch=false --browsers=ChromeHeadless",
    "test-all-libs-ci": "npm run test-angular-rating-ci && npm run test-angular-logging-ci",
    "pack-angular-logging": "cd dist/angular-console-logger && npm pack",
    "pack-angular-rating": "cd dist/angular-rating && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.3",
    "@angular/cli": "^17.1.3",
    "@angular/compiler-cli": "^17.1.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "19.5.0",
    "eslint": "^9.27.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "typescript": "~5.3.2",
    "typescript-eslint": "8.32.1"
  }
}
